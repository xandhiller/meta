source $HOME/Meta/configuration/aliases.sh
PATH+=:$HOME/Meta/scripts
# Anaconda stuff
PATH=$PATH:/opt/homebrew/anaconda3/bin/
alias python3='/opt/homebrew/anaconda3/bin/python3'
alias python='/opt/homebrew/anaconda3/bin/python'
# bindkey -v # Vim bindings in shell

# Plugins
source $HOME/Meta/configuration/antigen.zsh
antigen use oh-my-zsh # Use oh-my-zsh's library
antigen bundle git
antigen bundle zsh-users/zsh-autosuggestions
antigen bundle zsh-users/zsh-syntax-highlighting
antigen bundle softmoth/zsh-vim-mode
antigen apply # Tell antigen (package manager for zsh) that you're done


